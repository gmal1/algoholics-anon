// tsconfig from chrome team, minus strict rule, plus sourcemap
{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es5",
    "downlevelIteration": true,
    "module": "esnext",
    "jsx": "react",
    "jsxFactory": "h",
    "sourceMap": true,
    // "strict": true,
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "moduleResolution": "node",
    "baseUrl": "./",
    "types": []
  },
  "exclude": ["src/sw/**/*"]
}
